{
  "name": "ant-design-pro",
  "version": "1.0.0",
  "private": true,
  "description": "An out-of-box UI solution for enterprise applications",
  "scripts": {
    "analyze": "cross-env ANALYZE=1 umi build",
    "build": "umi build",
    "deploy": "cross-env ANT_DESIGN_PRO_ONLY_DO_NOT_USE_IN_YOUR_PRODUCTION=site npm run site && npm run gh-pages",
    "fetch:blocks": "pro fetch-blocks",
    "format-imports": "import-sort --write '**/*.{js,jsx,ts,tsx}'",
    "gh-pages": "cp CNAME ./dist/ && gh-pages -d dist",
    "i18n-remove": "pro i18n-remove --locale=zh-CN --write",
    "lint": "npm run lint:js && npm run lint:style && npm run lint:prettier",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src && npm run lint:style",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:prettier": "check-prettier lint",
    "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
    "prettier": "prettier -c --write \"**/*\"",
    "start": "umi dev",
    "start:no-mock": "cross-env MOCK=none umi dev",
    "test": "umi test",
    "test:all": "node ./tests/run-tests.js",
    "test:component": "umi test ./src/components",
    "ui": "umi ui"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged"
    }
  },
  "lint-staged": {
    "**/*.less": "stylelint --syntax less",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write",
      "git add"
    ],
    "**/*.{js,jsx}": "npm run lint-staged:js",
    "**/*.{js,ts,tsx}": "npm run lint-staged:js"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "dependencies": {
    "@ant-design/aliyun-theme": "^0.0.4",
    "@ant-design/colors": "^3.2.2",
    "@ant-design/dark-theme": "^1.0.3",
    "@ant-design/pro-layout": "^4.6.2",
    "@antv/data-set": "^0.10.2",
    "antd": "^3.25.1",
    "bizcharts": "^3.5.6",
    "camelcase-keys": "^6.1.1",
    "classnames": "^2.2.6",
    "codemirror": "^5.49.2",
    "dva": "^2.4.1",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "omit.js": "^1.0.2",
    "path-to-regexp": "^3.1.0",
    "poplar-annotation": "^2.0.0-beta3",
    "qs": "^6.9.1",
    "react": "^16.11.0",
    "react-codemirror2": "^6.0.0",
    "react-color": "^2.17.3",
    "react-copy-to-clipboard": "^5.0.2",
    "react-document-title": "^2.0.3",
    "react-dom": "^16.11.0",
    "react-fittext": "^1.0.0",
    "react-hotkeys": "^2.0.0",
    "react-markdown": "^4.2.2",
    "react-syntax-highlighter": "^11.0.2",
    "react-text-annotate": "^0.1.0",
    "redux": "^4.0.4",
    "slash2": "^2.0.0",
    "sunflower-antd": "^0.7.0",
    "umi": "^2.11.3",
    "umi-plugin-pro-block": "^1.3.6",
    "umi-plugin-react": "^1.13.3",
    "umi-request": "^1.2.9",
    "webpack-theme-color-replacer": "^1.3.2"
  },
  "devDependencies": {
    "@ant-design/pro-cli": "^1.0.14",
    "@types/classnames": "^2.2.9",
    "@types/express": "^4.17.2",
    "@types/history": "^4.7.3",
    "@types/jest": "^24.0.22",
    "@types/lodash": "^4.14.145",
    "@types/qs": "^6.5.3",
    "@types/react": "^16.9.11",
    "@types/react-document-title": "^2.0.3",
    "@types/react-dom": "^16.9.4",
    "@umijs/fabric": "^1.2.1",
    "chalk": "^2.4.2",
    "check-prettier": "^1.0.3",
    "cross-env": "^6.0.3",
    "cross-port-killer": "^1.2.1",
    "enzyme": "^3.10.0",
    "eslint": "5.16.0",
    "excel-loader": "^1.0.1",
    "express": "^4.17.1",
    "gh-pages": "^2.1.1",
    "husky": "^3.0.9",
    "import-sort-cli": "^6.0.0",
    "import-sort-parser-babylon": "^6.0.0",
    "import-sort-parser-typescript": "^6.0.0",
    "import-sort-style-module": "^6.0.0",
    "jest-puppeteer": "^4.3.0",
    "lint-staged": "^9.4.2",
    "mockjs": "^1.1.0",
    "node-fetch": "^2.6.0",
    "prettier": "^1.19.1",
    "pro-download": "1.0.1",
    "raw-loader": "^3.1.0",
    "stylelint": "^11.1.1",
    "umi-plugin-ga": "^1.1.5",
    "umi-plugin-pro": "^1.0.2",
    "umi-types": "^0.5.4"
  },
  "optionalDependencies": {
    "puppeteer": "^2.0.0"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "checkFiles": [
    "src/**/*.js*",
    "src/**/*.ts*",
    "src/**/*.less",
    "config/**/*.js*",
    "scripts/**/*.js"
  ]
}
